{
  "name": "tegel",
  "version": "0.0.7",
  "description": "Tegel Digital Design System",
  "main": "index.js",
  "scripts": {
    "build": "cd core && npm install && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-stencil:watch": "cd core && npm run build-stencil:watch",
    "build-storybook": "build-storybook --quiet",
    "deploy-storybook": "npm install && npm run build && npm run build-storybook",
    "start-storybook": "start-storybook -p 6006",
    "storybook": "concurrently --raw 'npm:build-stencil:watch' 'npm:start-storybook'",
    "commit": "cz",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scania-digital-design-system/tegel.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scania-digital-design-system/tegel/issues"
  },
  "homepage": "https://github.com/scania-digital-design-system/tegel#readme",
  "dependencies": {
    "addon-screen-reader": "^1.8.6"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "@storybook/addon-a11y": "^6.5.14",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/builder-webpack4": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addons": "^6.5.13",
    "@storybook/html": "^6.5.12",
    "storybook-addon-designs": "^6.3.1",
    "storybook-dark-mode": "^3.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "commitizen": "^4.2.4",
    "lint-staged": "^12.3.7",
    "husky": "^7.0.4",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "commitlint": "^15.0.0",
    "@commitlint/config-conventional": "^16.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "eslint",
    "**/*.scss": "stylelint --fix",
    "**/*.{js,jsx,ts,tsx,scss,md}": "prettier --write"
  },
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.10.0 <17.0.0"
  }
}
