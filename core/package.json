{
  "name": "@scania/tegel",
  "version": "0.0.72",
  "description": "Tegel Design System",
  "keywords": [
    "tegel",
    "scania",
    "design",
    "system"
  ],
  "homepage": "https://tegel.scania.com",
  "bugs": {
    "url": "https://github.com/scania-digital-design-system/tegel/issues"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/components/components.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scania-digital-design-system/tegel.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "build-stencil:watch": "stencil build --docs-readme --watch",
    "commit": "npm run --prefix ../ commit",
    "tegel-release": "./tegel-release.sh"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@stencil/core": "^3.0.0",
    "dotenv": "^16.0.3",
    "prettier": "^2.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@stencil/sass": "^2.0.1",
    "@types/jest": "^27.0.3",
    "addon-screen-reader": "^1.8.6",
    "@storybook/addon-docs": "^6.4.19",
    "babel-loader": "^8.2.5",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "puppeteer": "^10.0.0"
  },
  "license": "MIT",
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.10.0 <17.0.0"
  }
