{
  "name": "@scania/tegel-angular-17",
  "version": "1.13.0",
  "description": "Angular wrappers for Tegel package using Angular 17 and above",
  "main": "dist",
  "files": [
    "dist",
    "dist/package.json"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build components",
    "npm_pack": "cd dist/components && npm pack",
    "package": "npm run build && npm run npm_pack",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": false,
  "dependencies": {
    "tslib": "^2.3.0",
    "@scania/tegel": "1.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.0.0",
    "typescript": "~5.4.2"
  }
}
